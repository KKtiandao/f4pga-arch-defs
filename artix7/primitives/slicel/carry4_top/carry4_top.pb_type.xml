  </interconnect>
 </mode>
 <mode name="SPLIT">
  <xi:include href="carry4_split/carry4_split.pb_type.xml"/>
  <interconnect>
   <direct name="S"   input="BLK_IG-CARRY4.S"        output="BLK_IG-CARRY4_SPLIT.S"   />
