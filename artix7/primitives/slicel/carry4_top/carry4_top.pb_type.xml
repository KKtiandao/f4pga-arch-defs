 <input name="DI" num_pins="4"/>

 <output name="CO" num_pins="4"/>
 <output name="O"  num_pins="4"/>

 <input name="CIN"    num_pins="1"/>

 <mode name="COMPLETE">
  <xi:include href="carry4/carry4.pb_type.xml"/>
  <interconnect>
   <direct name="S"   input="BLK_IG-CARRY4.S"           output="BEL_BB-CARRY4.S"   />
   <direct name="DI"  input="BLK_IG-CARRY4.DI"          output="BEL_BB-CARRY4.DI"  />
   <direct name="CO"  input="BEL_BB-CARRY4.CO" output="BLK_IG-CARRY4.CO"           />
   <direct name="O"   input="BEL_BB-CARRY4.O"  output="BLK_IG-CARRY4.O"            />
